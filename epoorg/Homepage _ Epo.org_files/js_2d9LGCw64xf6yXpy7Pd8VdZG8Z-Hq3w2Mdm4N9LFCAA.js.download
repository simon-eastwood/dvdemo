(function ($, Drupal) {

  Drupal.behaviors.EpoShowSliderbar = {
    attach: function (context, settings) {
      let pixelsToScroll = 200;
      // Calculate total width of elements in the list.
      let widthOfList = function (list) {
        let itemsWidth = 0;
        $(list).find('li').each(function () {
          let itemWidth = $(this).outerWidth();
          itemsWidth += itemWidth;
        });
        return itemsWidth;
      };

      // Process visibility of controls.
      let reAdjust = function (animate = false) {
        let nav_tabs_container = $('.search-tabs, .bootstrap-tabs');
        $(nav_tabs_container).each(function () {
        
          // Add controls (left and right arrows).
          //$('.tabs .tabs-nav','.tabs .tabs-nav').once().prepend('<div class="scroller scroller-left"></div><div class="scroller scroller-right"></div>');
          $(this).find('.tabs-nav, .nav-tabs').once().before('<div class="scroller scroller-left"></div><div class="scroller scroller-right"></div>');
         
          let scroller_left = $(this).find('.scroller-left');
          let scroller_right = $(this).find('.scroller-right');
          let tabs = $(this).find('ul');
          let outerWidth = parseInt($(this).outerWidth());
          let width = parseInt(widthOfList(tabs));
          let left = parseInt($(tabs).scrollLeft());
          //scroller RIGHT show
          if ((left === 0) && ( left < (width - outerWidth)) ) {
            $(scroller_right).fadeIn('slow');
          } else if ( (left !== 0) && (left < (width - outerWidth )) ) {
            $(scroller_right).show();
          } else {
            $(scroller_right).fadeOut('slow');
          }
          //scroller LEFT show
          if ( (left > 0) || (left == (width - outerWidth)) )  {
            $(scroller_left).fadeIn('slow');
          }
          else {
            $(scroller_left).fadeOut('slow');
          }
        });
      }

      reAdjust();

      // Process windows resizing.
      $(window).once().on('resize', function (e) {
        reAdjust(true);
      });

      // Process re-adjust on scroll.
      $('.nav-tabs').scroll(function () {
        reAdjust(true);
      })

      // Process left arrow click.
      $('.scroller-left').once().click(function (e) {
        $(this).parent().find('.nav-tabs').animate({
          scrollLeft: '-=' + pixelsToScroll
        }, 'slow', function () {
          reAdjust(true);
        });
      });

      // Process right arrow click.
      $('.scroller-right').once().click(function (e) {
        $(this).parent().find('.nav-tabs').animate({
          scrollLeft: '+=' + pixelsToScroll
        }, 'slow', function () {
          reAdjust(true);
        });
      });

      // Fix incompatibility between lc_simple_tabs and layout_builder_tabs in the same page.
      // The first already manages visibility via 'active' class.
      // The second has too general selectors in tabs.js and affects first.
      $('.tab-content .block-inline-blocksimple-tabs-item.tab-pane').once().css('display', '');

    }
  };

})(jQuery, Drupal);
