  // Make keyboard accessibility for Language Dropdown
  Drupal.behaviors.LanguageDropdownKeys = {
    attach: function (context, settings) {
      let screenSizeDesktop = window.matchMedia('(min-width: 1500px)');
      // When the screen is above 1500px
    
      let focusElementsBig = $('.lang-dropdown-select .is-active').add('.language-switcher-language-url .multi-collapse li:not(".goBackLang") a');
       //targetting for a language menu item (eng/ger/french)

      let index = 0;
      //use index 

      if (screenSizeDesktop.matches) {
        $('.language-switcher-language-url').keydown(function (e) {
          startKeyboardEvents(e)
        });
      }

        function startKeyboardEvents(e){
          let dropdownLanguage = $('.language-switcher-language-url .multi-collapse');
          if ((dropdownLanguage).hasClass('show')) {
              switch (e.keyCode) {
                // DOWN OR PAGE DOWN
                case 40: case 34:
                  e.preventDefault();
                  key_down();
                  break;
                // UP OR PAGE UP 
                case 33 : case 38:
                  e.preventDefault();
                  key_up();
                  break;
                // ESC
                case 27:
                  $(dropdownLanguage).removeClass('show');
                  index = 0;
                  break;
                // TAB
                case 9:
                  e.preventDefault();
                  if(e.shiftKey) {
                    key_up();
                  } else {
                    key_down();
                  }
                  break;
                default:
               }             
  
                //add forcus to the selected element using a index
                 focusElementsBig.eq(index).focus();
              }
        }

        function key_down() {
          //update index to get the focus to NEXT element
          index = index + 1;
            if(index > (focusElementsBig.length - 1)) {
              index = 0;
            }
        }
        function key_up() {   
          //update index to get the focus to PREV element
          index = index - 1;
          if(index < 0) {
            index = 0;
          }
        }
      
      
    }
    
  };
 


